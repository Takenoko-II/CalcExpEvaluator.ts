{
    "include": ["src/"],
    "exclude": [],
    "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "module": "ES2022",
      "target": "ES2022",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strictNullChecks": true,
      "declaration": true,
      "strict": true,
      "noImplicitOverride": true,
      "rootDir": "src",
      "outDir": "dest"
    }
}
