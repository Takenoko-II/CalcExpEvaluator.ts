{
    "include": ["src/"],
    "exclude": [],
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "module": "ES2022",
        "target": "ES2022",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "declaration": true,
        "strict": true,
        "noImplicitOverride": true,
        "exactOptionalPropertyTypes": true,
        "allowUnusedLabels": false,
        "allowUnreachableCode": false,
        "noImplicitReturns": true,
        // "noUnusedParameters": true,
        // "noUnusedLocals": true,
        "rootDir": "src",
        "outDir": "dest"
    }
}
